{
  "name": "integration-huron-person",
  "version": "1.0.0",
  "description": "BU person integration via CDM api for Huron",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/types/bin/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && node bin/build.js",
    "build:tsc": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outdir dist/cjs",
    "pack": "npm run build && npm pack --pack-destination .",
    "start": "node dist/cjs/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "testsilent": "jest --silent",
    "clean": "rimraf dist"
  },
  "keywords": [
    "huron",
    "integration",
    "person"
  ],
  "author": "Warren Hennemuth",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "humanize-duration": "^3.33.1",
    "jsonwebtoken": "^9.0.2",
    "stream-json": "^1.9.1",
    "integration-core": "https://simple-npm-package-bucket.s3.us-east-1.amazonaws.com/integration-core-1.0.0.tgz"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/stream-json": "^1.7.8",
    "esbuild": "^0.25.10",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
